@section scripts{

    <script src="~/Scripts/custom/NotiClient.js"></script>
}

<div ng-app="NotiApp" class="container">
    <br />
    <br />
    <input type="text" placeholder="Search Student" ng-model="searchStudent" />
    <br />
    <div ng-controller="NotiController">
        <table class="table">
            <tr ng-repeat="r in notis | orderBy:'-'">
                <td>{{r.ID}}</td>
                <td>{{r.title}}</td>
                <td>{{r.desc}}</td>

            </tr>

        </table>

    </div>

</div>
